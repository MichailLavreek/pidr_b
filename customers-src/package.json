{
  "name": "localweb_client",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run start:serve",
    "start:serve": "ng serve --proxy-config proxy.conf.json --port 4200",
    "gulp:watch": "./node_modules/gulp/bin/gulp.js watch",
    "compile": "npm i && ng build --prod",
    "c": "npm run compile",
    "doc:gen": "./node_modules/.bin/compodoc -p tsconfig.json -d doc/client",
    "doc:serve": "node doc/index.js",
    "tests:unit": "./node_modules/karma/bin/karma start",
    "tests:e2e": "./node_modules/protractor/bin/protractor ./e2e/protractor.conf.js",
    "up": "npm run compile && npm run upload",
    "up38": "npm run compile && npm run upload38",
    "upForMac": "npm run compile && npm run uploadF",
    "upload": "npm run clearDeployDestinationFolder && npm run deploy",
    "upload38": "npm run clearDeployDestinationFolder38 && npm run deploy38",
    "uploadF": "npm run clearDeployDestinationFolderF && npm run deployF",
    "clearDeployDestinationFolder": "sshpass -f ./.sshDeployServerPassword ssh root@192.168.0.37 rm -rf /www/localweb/client",
    "clearDeployDestinationFolder38": "sshpass -f ./.sshDeployServerPassword38 ssh root@192.168.0.38 rm -rf /www/localweb/client",
    "clearDeployDestinationFolderF": "ssh root@192.168.0.37 rm -rf /www/localweb/client",
    "deploy": "sshpass -f ./.sshDeployServerPassword scp -r client root@192.168.0.37:/www/localweb/client",
    "deploy38": "sshpass -f ./.sshDeployServerPassword38 scp -r client root@192.168.0.38:/www/localweb/client",
    "deployF": "scp -r client root@192.168.0.37:/www/localweb/client"
  },
